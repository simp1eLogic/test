renameto -R '/ /_/' '*.mp3'
renameto -R '/,/_/' '*.mp3'
renameto -R '/—/-/' '*.mp3'
renameto -R '/["()*]//' '*.mp3'
renameto -R '/ /_/' '*.mp3.TStime'
renameto -R '/,/_/' '*.mp3.TStime'
renameto -R '/—/-/' '*.mp3.TStime'
renameto -R '/["()*]//' '*.mp3.TStime'
renameto -R -dir '/ /_/' '*'
renameto -R -dir '/,/_/' '*'
renameto -R -dir '/["()*]//' '*'
